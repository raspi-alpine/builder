ARCH=aarch64
SIZE_ROOT_FS=512M
SIZE_ROOT_PART=700M
ALPINE_BRANCH=3.18
ALPINE_MIRROR=http://uk.alpinelinux.org/alpine
CUSTOM_IMAGE_SCRIPT=image-no_mod.sh
CMDLINE=console=ttyAMA0,115200 console=tty1 root=/dev/root rootfstype=ext4 fsck.repair=yes ro rootwait quiet
ADDITIONAL_KERNEL_MODULES="g_ether dwc2 aes-neon-bs"
RPI_FIRMWARE_BRANCH=stable
CACHE_PATH=/cache
#DEFAULT_KERNEL_MODULES=*
STAGES=00 10 20 30 40 50 60 70 80 90 user-88
LIB_LOG=yes
OVERLAY=yes
